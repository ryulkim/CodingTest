#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;
    for (int i = 0; i < n; i++) {
        if (n % 2 == 1 && i == n - 1) {
            cout << 3;
        }
        else if (i % 2 == 0) {
            cout << 1;
        }
        else if (i % 2 == 1) {
            cout << 2;
        }
        cout << " ";
    }
}
