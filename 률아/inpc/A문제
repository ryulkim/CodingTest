#include <iostream>
using namespace std;

int main() {
    int a, b, c;
    int sum = 1;
    cin >> a >> b >> c;
    if (a % 2 == 1) {
        sum *= a;
    }
    if (b % 2 == 1) {
        sum *= b;
    }
    if (c % 2 == 1) {
        sum *= c;
    }
    if (a % 2 == 0 && b % 2 == 0 && c % 2 == 0) {
        sum = a * b * c;
    }
    cout << sum;
}
